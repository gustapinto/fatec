Algoritmo "convertedolarprareal"

Var
      COTACAO, DOLARES, REAIS : real
Inicio
      escreva("Cotação do Dólar hoje: ")
      leia(COTACAO)
      
      escreva("Dólares: ")
      leia(DOLARES)
      
      REAIS <- DOLARES * COTACAO
      
      escreva("Reais: R$", REAIS)
Fimalgoritmo